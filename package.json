{
  "name": "m9-generator",
  "version": "0.0.1",
  "description": "Static Site Generator",
  "bin": {
    "m9": "./bin/m9.js"
  },
  "babel": {
    "extends": "./.babelrc-generator"
  },
  "scripts": {
    "m9": "./bin/m9.js --src=docs-src --dst=docs --m9-env=development",
    "m9-tasks": "./bin/m9.js --tasks --src=docs-src --m9-env=development",
    "build": "npm run build-dist && npm run build-docs",
    "build-dist": "rm -rf dist/* && babel -s inline -d dist src",
    "build-docs": "NODE_ENV=production ./bin/m9.js build --src=docs-src --dst=docs",
    "dev": "./bin/m9.js dev --src=docs-src --dst=docs --m9-env=development",
    "test": "npm run test-src && npm run test-docs",
    "test-src": "standard --verbose src/*.js src/**/*.js | snazzy",
    "test-docs": "./bin/m9.js test --src=docs-src --m9-env=development"
  },
  "author": "Aleksandr Tsertkov <tsertkov@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "marked": "^0.4.0",
    "snazzy": "^7.1.1"
  },
